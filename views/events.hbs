                            <div class="meeting-panel2 col-md-6 col-sm-12">
                                <div class="row">
                                    <div class="meeting-panel2a">
                                        <input class="dp" id="meeting-date" placeholder="Date">
                                        <input id="mstart" placeholder="Start">
                                        <input id="mend" placeholder="End">    
                                    </div>
                                    <div class="meeting-panel2b">
                                        <input id="meeting-location" placeholder="Location">    
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-sm-12">
                                <div class="row">
                                    <div class="meeting-panel3">
                                        <textarea id="meeting-desc" placeholder="Description"></textarea>
                                    </div>
                                </div>
                            </div>
                                
                        </div>
